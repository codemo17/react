{
  "source": "./src",
  "destination": "./.dist/doc",
  "includes": ["\\.(js|jsx)$"],
  "lint": true,
  "coverage": true,
  "test": {
    "type": "mocha",
    "source": "./src",
    "includes": ["(spec|test).(js)$"]
  },
  "experimentalProposal": {
    "classProperties": true,
    "objectRestSpread": true,
    "decorators": true,
    "doExpressions": true,
    "functionBind": true,
    "asyncGenerators": true,
    "exportExtensions": true,
    "dynamicImport": true
  }
}

